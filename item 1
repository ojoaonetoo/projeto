if(val > 10 ){
  alert("Valor é maior que 10!")
} else if(val < 10 && val >= 1){
  alert("Valor é Menor que 10!")
}else if(val = 10){
  alert("Valor é 10")
}else{
  alert("Valor nao existente!")
} 

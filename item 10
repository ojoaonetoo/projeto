<script>
var val = 1
var dral = 10
document.write("valores de 1 a 10 em ordem!")
  while(val <= 10){
    document.write("<br>"+ val);
    val++
  }
  while(dral >= 1){
    document.write("<br>"+ dral)
    dral--
  }
</script>
